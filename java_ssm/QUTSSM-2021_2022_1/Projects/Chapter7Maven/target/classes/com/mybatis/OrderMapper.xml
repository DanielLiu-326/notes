<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.OrderDao">
	<!-- 根据用户uid查询订单信息 -->
	<select id="selectOrderById" parameterType="Integer" resultType="com.po.Order">
		select * from order1 where user_id=#{uid}
	</select>
</mapper>